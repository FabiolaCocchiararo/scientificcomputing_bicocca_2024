@Article{	  kazanas1997,
  abstract	= {We have found relations between the temporal and spectral
		  properties of radiation Comptonized in an extended
		  atmosphere associated with compact accreting sources. We
		  demonstrate that the fluctuation power spectrum density
		  (PSD) imposes constraints on the atmosphere scale and
		  profile. Furthermore, we indicate that the slope and
		  low-frequency break of the PSD are related to the Thomson
		  depth, tau 0, of the atmosphere and the radius of its
		  physical size, respectively. Since the energy spectrum of
		  the escaping radiation also depends on tau 0 (and the
		  electron temperature kTe), the relation between spectral
		  and temporal properties follows. This relation allows, for
		  the first time, an estimate of the accreting matter Thomson
		  depth, tau 0, independent of arguments involving
		  Comptonization. We present figures for the light curves and
		  PSDs of different energy bands, the photon energy spectra,
		  and the phase lags as functions of the variability
		  frequency. The temporal properties of the high (soft) and
		  low (hard) state of black hole sources are discussed in
		  this context.},
  adsnote	= {Provided by the SAO/NASA Astrophysics Data System},
  adsurl	= {http://ads.nao.ac.jp/abs/1997ApJ...480..735K},
  author	= {{Kazanas}, D. and {Hua}, {X.-M.} and {Titarchuk}, L.},
  doi		= {10.1086/303991},
  journal	= {\apj},
  keywords	= {ACCRETION, ACCRETION DISKS, BLACK HOLE PHYSICS, RADIATION
		  MECHANISMS: NONTHERMAL, STARS: NEUTRON},
  month		= may,
  owner		= {tmd},
  pages		= {735-+},
  timestamp	= {2009.12.15},
  title		= {{Temporal and Spectral Properties of Comptonized Radiation
		  and Its Applications}},
  volume	= {480},
  year		= {1997},
  bdsk-url-1	= {http://dx.doi.org/10.1086/303991}
}

